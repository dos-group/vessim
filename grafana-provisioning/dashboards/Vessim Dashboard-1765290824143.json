          "query": "from(bucket: \"vessim_bucket\")\r\n  |> range(start: 0)\r\n  |> filter(fn: (r) => r._measurement == \"vessim\")\r\n  // Hier nutzen wir deine funktionierende Variable\r\n  |> filter(fn: (r) => r.microgrid == \"${microgrid}\")\r\n  // Wir nehmen p dazu, damit wir die Leistung sehen\r\n  |> filter(fn: (r) => r._field == \"latitude\" or r._field == \"longitude\" or r._field == \"p\")\r\n  |> last()\r\n  |> group(columns: [\"_measurement\"])\r\n  |> map(fn: (r) => ({ r with actor: if exists r.actor then r.actor else \"Microgrid\" }))\r\n  |> pivot(\r\n      rowKey: [\"_time\", \"microgrid\", \"actor\"],\r\n      columnKey: [\"_field\"],\r\n      valueColumn: \"_value\"\r\n  )\r\n  |> group()\r\n  // Wir behalten p im keep, damit du es visualisieren kannst\r\n  |> keep(columns: [\"microgrid\", \"actor\", \"latitude\", \"longitude\", \"p\"])",
          "refId": "A"
        }
